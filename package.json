    "ios": "react-native run-ios --simulator=\"iPhone 13 Pro Max\"",
    "ios-release": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --configuration Release",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
